<%= form_for @wiki do |f| %>
  Title: <%= f.text_field :title %>
  <br/>
  Body: <%= f.text_field :body %>
  <br/>
  <% if current_user.role == "premium" %>
    Private: <%= f.check_box :private %>
  <% end %>
  <br/>
  Owner:
  <%= f.collection_select :user_id, User.all, :id, :email %>
  <Br/>
  <%= f.submit %>

<% end %>
